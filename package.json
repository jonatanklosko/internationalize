{
  "name": "internationalize",
  "version": "1.0.0",
  "description": "A web application simplifying translation of locale files from one language to another.",
  "homepage": "http://internationalize.herokuapp.com",
  "keywords": [
    "i18n",
    "internationalization",
    "translation"
  ],
  "author": "Jonatan KÅ‚osko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonatanklosko/internationalize.git"
  },
  "bugs": "https://github.com/jonatanklosko/internationalize/issues",
  "scripts": {
    "start": "npm run server",
    "postinstall": "npm run build",
    "server": "node server/server.js",
    "server:watch": "nodemon --watch server server/server.js",
    "build": "webpack --progress",
    "wdserver": "webpack-dev-server --inline --hot --open",
    "lint": "eslint . --ignore-path .gitignore",
    "test:server": "node spec/server/spec-runner.js",
    "test:server:watch": "nodemon --watch server --watch spec/server spec/server/spec-runner.js",
    "test:angular-unit": "karma start spec/angular-unit/karma.conf.js --single-run",
    "test:angular-unit:watch": "karma start spec/angular-unit/karma.conf.js --auto-watch",
    "test:e2e": "protractor spec/e2e/protractor.conf.js",
    "pre:test:e2e": "SERVER_URL=http://localhost:3001 webpack-dev-server --host localhost --port 3002",
    "webdriver:update": "webdriver-manager update"
  },
  "engines": {
    "node": "6.7.0"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-clipboard": "^1.5.0",
    "angular-material": "^1.1.1",
    "angular-ui-router": "^0.3.1",
    "babel-polyfill": "^6.16.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.15.2",
    "connect-mongodb-session": "^1.3.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "http-status": "^0.2.3",
    "jquery": "^3.1.1",
    "js-sha1":"^0.4.1",
    "js-yaml": "viroulep/js-yaml#dump-hash",
    "mongoose": "^4.7.0",
    "mongoose-unique-validator": "^1.0.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "prismjs": "^1.6.0",
    "superagent": "^2.2.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "app-module-path": "^1.1.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "clean-webpack-plugin": "^0.1.13",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "factory-girl": "^4.0.0-beta.5",
    "favicons-webpack-plugin": "0.0.7",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jasmine": "^2.5.0",
    "jasmine-promises": "^0.4.1",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.26",
    "karma-webpack": "^1.8.0",
    "ng-annotate-loader": "^0.2.0",
    "ngtemplate-loader": "^1.3.1",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "protractor": "^4.0.9",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "superagent-prefix": "^0.0.2",
    "superagent-use": "^0.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
